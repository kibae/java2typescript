{
    "name": "java2ts",
    "version": "0.1.0",
    "description": "Convert Java Code to Typescript",
    "keywords": [
        "Java",
        "Typescript",
        "ANTLR4"
    ],
    "type": "commonjs",
    "author": "Mike Lischke",
    "license": "MIT",
    "dependencies": {
        "antlr4-c3": "2.2.1",
        "antlr4ts": "0.5.0-alpha.4",
        "fast-printf": "1.6.9",
        "glob": "8.0.3",
        "hash-sum": "2.0.0",
        "printf": "0.6.1"
    },
    "devDependencies": {
        "@types/glob": "^8.0.0",
        "@types/jest": "^29.0.0",
        "@types/node": "^18.7.16",
        "@typescript-eslint/eslint-plugin": "^5.37.0",
        "@typescript-eslint/eslint-plugin-tslint": "^5.37.0",
        "@typescript-eslint/parser": "^5.37.0",
        "antlr4ts-cli": "^0.5.0-alpha.4",
        "eslint": "^8.23.1",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-jsdoc": "^39.3.6",
        "eslint-plugin-prefer-arrow": "^1.2.3",
        "jest": "^29.0.3",
        "ts-jest": "^29.0.1",
        "typescript": "4.8.3"
    },
    "scripts": {
        "test": "jest --testMatch [ \"**/test/**/*.spec.ts\" ] --no-coverage",
        "test-coverage": "jest  --testMatch [ \"**/test/**/*.spec.ts\" ] --coverage",
        "lint": "eslint \"./src/**/*.ts\"",
        "antlr4ts": "antlr4ts -Xexact-output-dir ./java/JavaLexer.g4 ./java/JavaParser.g4 -visitor -o ./java/generated"
    },
    "jest": {
        "collectCoverageFrom": [
            "lib/**/*.ts"
        ],
        "coverageReporters": [
            "json",
            "text",
            "clover",
            "html"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 32,
                "branch": 14,
                "functions": 18,
                "lines": 32
            }
        },
        "preset": "ts-jest"
    }
}
