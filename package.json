{
    "name": "java2ts",
    "version": "0.1.0",
    "description": "Convert Java Code to Typescript",
    "keywords": [
        "Java",
        "Typescript",
        "ANTLR4"
    ],
    "author": "Mike Lischke",
    "license": "MIT",
    "type": "module",
    "bin": {
        "java2ts": "output/src/convert.js"
    },
    "dependencies": {
        "antlr4-c3": "3.0.1",
        "antlr4ts": "0.5.0-alpha.4",
        "glob": "9.3.2",
        "jree": "1.2.2"
    },
    "devDependencies": {
        "@types/glob": "8.1.0",
        "@types/jest": "29.5.0",
        "@types/jsdom": "21.1.1",
        "@types/node": "18.15.10",
        "@typescript-eslint/eslint-plugin": "5.57.0",
        "@typescript-eslint/parser": "5.57.0",
        "antlr4ts-cli": "0.5.0-alpha.4",
        "eslint": "8.37.0",
        "eslint-plugin-import": "2.27.5",
        "eslint-plugin-jsdoc": "40.1.0",
        "eslint-plugin-prefer-arrow": "1.2.3",
        "jest": "29.5.0",
        "ts-jest": "29.0.5",
        "ts-node": "10.9.1",
        "typescript": "5.0.2"
    },
    "scripts": {
        "build": "npm run generate && tsc",
        "fetch-apis": "ts-node-esm tools/fetchJavaApis.ts",
        "run-antlr4-conversion": "ts-node-esm  ./src/convert.ts config/antlr4.json",
        "test": "jest --config=./tests/jest.config.ts --no-coverage --watchAll=false",
        "test-ci": "jest --config=./tests/jest.config.ts --no-coverage --watchAll=false --silent",
        "test-coverage": "jest --config=./tests/jest.config.ts --coverage --silent",
        "lint": "eslint \"./src/**/*.ts\"",
        "generate": "antlr4ts -Xexact-output-dir ./parser/JavaLexer.g4 ./parser/JavaParser.g4 -visitor -o ./parser/generated"
    }
}
