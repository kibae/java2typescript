{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Run current Jest test",
            "runtimeExecutable": null,
            "runtimeArgs": [
                "--inspect-brk",
                "${workspaceRoot}/node_modules/.bin/jest",
                "--runInBand",
                "${fileBasenameNoExtension}.js",
            ],
            "console": "integratedTerminal",
            "stopOnEntry": false,
            "preLaunchTask": "tsc",
            "outFiles": [
                "${workspaceFolder}/output/src/**/*.js"
            ],
            "env": {
                "NODE_ENV": "testing"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch converter",
            "program": "${workspaceFolder}/src/main-antlr4.ts",
            "outFiles": [
                "${workspaceFolder}/output/**/*.js"
            ],
            "sourceMaps": true,
            "stopOnEntry": false,
            "smartStep": true,
            "args": [],
            "runtimeArgs": [
                "--nolazy"
            ],
            "console": "integratedTerminal",
            "preLaunchTask": "tsc: watch",
            "trace": false
        },
    ]
}
