{
    "version": "0.2",
    "words": [
        "antlr",
        "Appendable",
        "Cloneable",
        "closeables",
        "Declarators",
        "instanceof",
        "misformatted",
        "signum",
        "subfolders",
        "whitespaces"
    ],
    "ignoreWords": [
        "COLONCOLON",
        "LBRACK",
        "LPAREN",
        "RBRACK",
        "RPAREN",
        "javax",
        "Xexact",
        "arraycopy",
        "stringbuilder",
        "stringtemplate"
    ],
    "ignorePaths": [
        ".eslintrc.json"
    ]
}
